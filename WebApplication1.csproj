    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>4021c9e4-583e-4751-a3fa-2ea0dbfd8f7e</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
